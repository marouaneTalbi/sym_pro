        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>

        {% include 'nav.html.twig' %}
<div class="container">

{% for msg in app.flashes('MsgSuccess') %}
     <div class="alert alert-success text-center">{{msg}}</div>   
{% endfor %}


        {% block body %}
        {% endblock %}

</div>
    </body>
